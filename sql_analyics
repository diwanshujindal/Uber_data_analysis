CREATE OR REPLACE TABLE `ethereal-reef-411521.Uber_data.tbl` AS (
SELECT 
f.trip_id,
f.VendorID,
d.tpep_pickup_datetime,
d.tpep_dropoff_datetime,
p.passenger_count,
t.trip_distance,
r.rate_code_name,
pick.pickup_latitude,
pick.pickup_longitude,
drop.dropoff_latitude,
drop.dropoff_longitude,
pay.payment_type_name,
f.fare_amount,
f.extra,
f.mta_tax,
f.tip_amount,
f.tolls_amount,
f.improvement_surcharge,
f.total_amount
FROM 

`ethereal-reef-411521.Uber_data.fact_table` f
JOIN `ethereal-reef-411521.Uber_data.datetime` d  ON f.datetime_id=d.datetime_id
JOIN `ethereal-reef-411521.Uber_data.passenger_count` p  ON p.passenger_count_id=f.passenger_count_id  
JOIN `ethereal-reef-411521.Uber_data.trip_distance` t  ON t.trip_distance_id=f.trip_distance_id  
JOIN `ethereal-reef-411521.Uber_data.rate_code` r ON r.rate_code_id=f.rate_code_id  
JOIN `ethereal-reef-411521.Uber_data.pickup_location` pick ON pick.pickup_location_id=f.pickup_location_id
JOIN `ethereal-reef-411521.Uber_data.dropoff_location` drop ON drop.dropoff_location_id=f.dropoff_location_id
JOIN `ethereal-reef-411521.Uber_data.payment_type` pay ON pay.payment_type_id=f.payment_type_id)
;
